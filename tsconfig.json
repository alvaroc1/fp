{
  "compilerOptions": {
    "strict": true,
    "module": "CommonJS",
    "jsx": "react",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,

    // false if source maps are disabled
    "sourceMap": true,
    "noEmit": false,
    "lib": [ "dom", "es2019.object", "es2017.object", "es6" ]
  }
}